    <div class="br-mainpanel"></div>
      <div class="pd-30">
        <h4 class="tx-gray-800 mg-b-5">Department Registration</h4>
@*        <p class="mg-b-0">Welcome to User Department Dashboard</p>*@
      </div><!-- d-flex -->

      <div class="br-pagebody mg-t-5 pd-x-30">
          @{
                        foreach(var item in Model)
                        {
       <div class="row mg-b-25">
             
        <div class="col-lg-3" style="cursor:pointer">
            <div class="bg-teal rounded overflow-hidden">
                <div class="pd-25 d-flex align-items-center">
                    <i class="ion ion-android-document tx-60 lh-0 tx-white op-7"></i>
                    <div class="mg-l-20">
                        <p class="tx-10 tx-spacing-1 tx-mont tx-medium tx-uppercase tx-black mg-b-10">
                            Total Registration
                        </p>
                        
                        <p class="tx-24 tx-white tx-lato tx-bold mg-b-2 lh-1">@item.totalreg</p>
                                     
                    </div>
                </div>
            </div>
        </div>
       
         <!-- col-3 -->
        <div class="col-lg-3" style="cursor:pointer;background-color:#90d6a2">
            <div class="bg-teal rounded overflow-hidden" style="background-color:#90d6a2">
                <div class="pd-25 d-flex align-items-center" style="background-color:#90d6a2">
                <i class="ion ion-android-document tx-60 lh-0 tx-white op-7"></i>
                <div class="mg-l-20">
                  <p class="tx-10 tx-spacing-1 tx-mont tx-medium tx-uppercase tx-black mg-b-10">
					Total Approved</p>
                       
                            <p class="tx-24 tx-white tx-lato tx-bold mg-b-2 lh-1">@item.approved</p>
                        

                </div>
              </div>
            </div>
          </div>
          
		  <!-- col-3 -->
           <div class="col-lg-3" style="cursor:pointer">
            <div class="bg-teal rounded overflow-hidden">
              <div class="pd-25 d-flex align-items-center">
                <i class="ion ion-android-document tx-60 lh-0 tx-white op-7"></i>
                <div class="mg-l-20">
                  <p class="tx-10 tx-spacing-1 tx-mont tx-medium tx-uppercase tx-black mg-b-10">
					Total Pending</p>
                  <p class="tx-24 tx-white tx-lato tx-bold mg-b-2 lh-1">@item.pending</p>
                  
                </div>
              </div>
            </div>
          </div>

        <div class="col-lg-3" style="cursor:pointer;background-color:#90d6a2">
            <div class="bg-teal rounded overflow-hidden" style="background-color:#90d6a2">
                <div class="pd-25 d-flex align-items-center" style="background-color:#90d6a2">
                    <i class="ion ion-android-document tx-60 lh-0 tx-white op-7"></i>
                    <div class="mg-l-20">
                        <p class="tx-10 tx-spacing-1 tx-mont tx-medium tx-uppercase tx-black mg-b-10">
                            Total Reject
                        </p>
                        <p class="tx-24 tx-white tx-lato tx-bold mg-b-2 lh-1">@item.reject</p>

                    </div>
                </div>
            </div>
        </div>
       
        
		  </div>
           }
        }

