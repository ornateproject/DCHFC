@model ssc.Models.DBModel
@*
For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@

@{
    Layout = null;

}
@*=================================================Main
Code=========================================================================*@
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

@*=================================================Main
Code=========================================================================*@
<html>

<head>
</head>

<body>
    @*=================================================Header=========================================================================*@
    <form asp-action="CandidateSheet" asp-controller="DB">
    <div class="br-pagebody">
        <div class="br-section-wrapper">
            <div class="pd-x-20 pd-sm-x-30 pd-t-20 pd-sm-t-30 text-center">
                <img src="~/img/logo.jpg" /><br />
                <h5 class="fw-bold" style="color:black">STAFF SELECTION COMMISSION</h5>
                <p style="color:black"><b><u>Combined Higher Secondary (10+2) Level Examination . 2021</u></b></p>
                <h5 style="color:black">ONLINE DATA VERIFICATION SHEET</h5>
                @*======================================Table area==============================*@
                <div class="container mt-3 mr-3 mf-3">
                    <div class="table-responsive">
                        @*==============================================first Table=======================================*@
                        <table class="table table-bordered">

                            <tbody>
                                <tr>
                                    <td width="10px">1.1</td>
                                    <td width="100%">Roll no.</td>
                                    <td><input style="border:none" asp-for="@Model.Reg_no"  type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.2</td>
                                    <td width="100%">Name.</td>
                                    <td><input style="border:none" asp-for="@Model.Name" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.3</td>
                                    <td width="100%">Remarks with respect to the Candidate Name</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Candidate_remark" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.4</td>
                                    <td width="100%">Father name</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Father_name" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.5</td>
                                    <td width="100%">Remarks with respect to the Father name .</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="father_remark" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.6</td>
                                    <td width="100%">Mother name</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Mother_name" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.7</td>
                                    <td width="100%">Remarks with respect to the Mother Name</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Mother_remark" type="text" /></td>
                                <tr>
                                    <td width="10px">1.8</td>
                                    <td width="100%">
                                        Matriculation Roll No.
                                        ( As mentioned in the Application Form)
                                    </td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Matric_rollno" type="text" /></td>
                                <tr>
                                    <td width="10px">1.9</td>
                                    <td width="100%">Gender</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Gender" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.10</td>
                                    <td width="100%">Category(Cat 1)</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Category_cat1" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.11</td>
                                    <td width="100%">Reason for change in category , if any.</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Reason_changecategory" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12</td>
                                    <td width="100%">Whether EX-Serviceman(ESM)(Cat2)</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="ESM_cat2" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.1</td>
                                    <td width="100%">Date of joining the Armed Force</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="AF_joining" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.2</td>
                                    <td width="100%">Date of Discharge</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Date_discharge" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.3</td>
                                    <td width="100%">Length of Service.</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Lenght_service" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.4</td>
                                    <td width="100%">
                                        Have you joined a Government job on civil side (including public<br />
                                        Sector Undertaking , Autonomous Bodies /Statutory Bodies <br />
                                        Nationalized Bank etc.) on regular basis after availing of the benefits<br />
                                        of reservation given to ex-serviceman for reemployment
                                    </td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Gov_civilside" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.5</td>
                                    <td width="100%">Date of Joining of the Civil Post</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Civilpost_joining" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.6</td>
                                    <td width="100%">Date of submission of application form</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Formsubmission_date" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.7</td>
                                    <td width="100%">
                                        Have you submitted the self-declaration /undertaking to your current<br />
                                        employer that you have applied for this examination
                                    </td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Self_declaration" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.12.8</td>
                                    <td width="100%">
                                        Wheather eligible for the posts reserved for ESM (Depending on re-<br />
                                        employment in Govt./Semi-Govt organzation
                                        )
                                    </td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Reservedpost_ESM" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.13 And 1.14 </td>
                                    <td width="100%">Weather person with Disability (PwD)</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Persondisability_PWD" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.15 </td>
                                    <td width="100%">Region Code</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Region_Code" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.16</td>
                                    <td width="100%">Exemption from Typewriting Test</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="Typewriting_test" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.17</td>
                                    <td width="100%">DV schedule Date</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="DVschedule_date" type="text" /></td>
                                </tr>
                                <tr>
                                    <td width="10px">1.17</td>
                                    <td width="100%">DV Actual Date</td>
                                    <td><input style="border:none;background-color:#FFF" asp-for="DVactual_date" type="text" /></td>
                                </tr>
                            </tbody>
                        </table>
                        @*============================Second Table=========================*@

                        <table class="table table-bordered m-3">
                            <tbody>
                                <tr>
                                    <td colspan="6" style="text-align: center;"><b>PART - II (Education Details)</b></td>
                                </tr>
                                <tr>
                                    <td width="20px" colspan="3">2.1 Highest Education Qualification(As declared by the candidate)</td>
                                    <td colspan="3"><input style="border:none;background-color:#FFF" asp-for="highest_qualification" type="text" /></td>
                                </tr>
                                <tr>
                                    <td colspan="3">2.2 Qualifying Education Qualification</td>
                                    <td colspan="3"><input style="border:none;background-color:#FFF" asp-for="Qualifying_education" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>State/UT of <br /> Board/University</td>
                                    <td>Name of Board/University</td>
                                    <td>Subject/Branch</td>
                                    <td>Roll No</td>
                                    <td>Year of passing </td>
                                    <td>percentage</td>
                                    <td>CGPA</td>
                                </tr>
                                <tr>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="University" type="text" rows="2" cols="12" /></td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Nameof_board" type="text" rows="2" cols="20" /></td>
                                    <td> <input style="border:none;background-color:#FFF" type="text" asp-for="Subjects" rows="2" cols="12" /></td>
                                    <td> <input style="border:none;background-color:#FFF" type="text" asp-for="Rollno_highestqualification" rows="2" cols="12" /></td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Passing_Year" type="text" rows="2" cols="8" /></td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Percentage" type="text" rows="2" cols="5" /></td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="CGPA" type="text" rows="2" cols="5" /></td>

                                </tr>
                            </tbody>
                        </table>
                        @*==========================================three table======================================================*@
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <td colspan="2" style="text-align: center;"><b>PART - III (Details of Option Form)</b></td>
                                </tr>
                                <tr>
                                    <td>3.1 Code for Eligible Post</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Eligiblepost_code" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>3.2 Order of preference</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Order_prefrence" type="text" rows="2" cols="20"/></td>
                                </tr>
                            </tbody>
                        </table>
                        @*==========================================forth table===============================================================*@

                        <table class="table table-bordered">
                            <tbody>


                                <tr>
                                    <td colspan="2" style="text-align: center;"><b>PART - IV (Date of Birth)</b></td>
                                </tr>
                                <tr>
                                    <td>4.1 Post Prefrence</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Post_prefrence" type="text" rows="2" cols="30" /></td>
                                </tr>
                                <tr>
                                    <td>4.2 Date of Birth(dd/mm/yyyy)</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="DOB" type="text" rows="2" cols="30" /></td>
                                </tr>
                                <tr>
                                    <td>4.3 Age(as on 01.01.2022)</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="AGE" type="text" rows="2" cols="30" /></td>
                                </tr>
                                <tr>
                                    <td>4.4 Age Relaxation Code</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Relaxation_age" type="text" rows="2" cols="30" /></td>
                                </tr>
                            </tbody>
                        </table>
                        <p><b>PART - V (DOCUMENT CHECKING WITH APPLICATION)</b></p>
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <td style="text-align: center;"><b>PARTICULARS</b></td>
                                    <td style="text-align: center;"><b>OPTION</b></td>
                                    <td style="text-align: center;"><b>Date of ISSUE</b></td>

                                </tr>
                                <tr>
                                    <td>
                                        5.1 Whether Approved by Distance Education Bureau ,UGC in<br />
                                        case of education through Distance Mode
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="Distance_education" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>5.2 Whether document in support of DOB produced</td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="Document_DOB" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>
                                        5.3 Whether possesses Essential Qualification within the</br>
                                        stipulated date
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="Essential_qualification" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>5.4 Whether document in support of caste produced</td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="Document_caste" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>
                                        5.5 Whether OBC certificate issued from the component<br />
                                        authority in prescribed proforma produced
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="OBC_certificate" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>
                                        5.6 Whether ESM discharge certified issued within the <br />
                                        stipulated date and whether submitted undertaking
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="ESMdischarge_certificate" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>
                                        5.7 Wherther PwD Status as claimed by the candidate<br />
                                        estabish from the certified produced
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="PWD_status" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>
                                        5.8 Whether NOC produced in case of Central Govt.Civilian<br />
                                        Employee candidates who have claimed age-relaxation
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="NOCproduced_Central" style="border:none" type="text" /></td>
                                </tr>
                                <tr>
                                    <td>
                                        5.9 Whether central Govt.Civilian employee status<br />
                                        establis
                                    </td>
                                    <td width="10px"><input width="10px" style="border:none" type="text" /></td>
                                    <td width="10px"><input width="10px" asp-for="Civilianemployee_status" style="border:none" type="text" /></td>
                                </tr>
                            </tbody>
                        </table>
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <td colspan="2" style="text-align: center;"><b>PART - VI (Rejection Provision)</b></td>
                                </tr>
                                <tr>
                                    <td>6.1 Candidate Status</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Candidature_status" type="text" rows="2" cols="60" /></td>
                                </tr>
                                <tr>
                                    <td>6.2 Reason for Rejection</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Reject_reason" type="text" rows="2" cols="60" /></td>
                                </tr>
                                <tr>
                                    <td>6.3 Remark</td>
                                    <td> <input style="border:none;background-color:#FFF" asp-for="Remarks" type="text" rows="2" cols="60" /></td>
                                </tr>
                            </tbody>

                        </table>
                        <div style="display:flex;justify-content: space-evenly;">
                            <b style="margin-top:100px;">Photo</b>
                            <span style="border: 1px solid black">
                                <img src="~/img/avatar.jpeg" height="200px" width="200px" />
                            </span>
                            <b style="margin-top:100px;">Thumb Impression</b>
                            <span style="border: 1px solid black">
                                <img src="~/img/thumbJPG.jpeg" height="200px" width="200px" />
                            </span>
                            <b style="margin-top:100px;">Signature</b>
                            <span style="border: 1px solid black">
                                <img src="~/img/sign.png" height="100px" width="200px" />
                            </span>
                        </div>
                        <div style="display:flex;justify-content: space-evenly; margin-top:50px;">
                            <b style="margin-top:100px;">Application Photo</b>
                            <span style="border: 1px solid black; padding:100px">

                            </span>
                            <b style="margin-top:100px;">Application Signature</b>
                            <span style="border: 1px solid black;padding:100px">

                            </span>
                        </div>
                        <div style="margin-top:30px;margin-bottom:10px;text-align:left">
                            The paticular as mentioned here in above regarding my candiature have been checked by me and found to
                            be correct. Preferences for posts/departments as shown above have been exerised by me consciously and
                            carefully and the same are to be treated as final. I also understand that request for any change in the
                            above particular including preferences would not be entertained by the Commission.
                        </div>
                        <div style="display:flex;justify-content: space-evenly;">


                            <b style="margin-top:30px;">
                                Signature OF <br />Candidate
                            </b>
                            <span style="border: 1px solid black;width:250px;height:50px;margin-top:20px;">

                            </span>

                             <b style="margin-top:30px;">
                                Signature OF <br />Document Checker
                            </b>
                            <span style="border: 1px solid black;width:250px;height:50px;margin-top:20px;">

                            </span>
                        </div>
                        <div style="display:flex;justify-content: left;">
                            <b style="margin-top:50px;">
                                Name OF Candidate
                            </b>
                            <span style="width:250px;height:50px;margin-top:20px;">

                            </span>
                        </div>
                        <div style="display:flex;justify-content: space-evenly;">


                            <b style="margin-top:40px;">
                                DATED:
                            </b>
                            <span style="width:250px;height:50px;margin-top:20px;">

                            </span>

                            <b style="margin-top:30px;">
                                Signature of Authorised<br/>
                                Officer of SSC
                            </b>
                            <span style="border: 1px solid black;width:250px;height:50px;margin-top:20px;">

                            </span>
                        </div>
                        <div style="display:flex;justify-content: right;margin-right:95px">
                            <b style="margin-top:50px;margin-right:70px;">
                                (Name of Officer)
                            </b>
                            <span style="border: 1px solid black;width:250px;height:50px;margin-top:20px;">

                            </span>
                        </div>
                    </div>
                    <div style="padding:30px">
                   <button  style="padding:10px" type="submit"  id="popup-btn" class="btn btn-red" > Apply
                                        </button>
                    </div>
                </div>
                <div>
                </div>
            </div>
            <div>


                @*==============================================Main css============================================*@

            </div>
        </div>
    </div>
    </form>
</body>

</html>
@*===============Script to print html=============*@

<script>
    function printpage() {
        window.print();
    }
</script>